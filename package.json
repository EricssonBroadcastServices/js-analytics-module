{
  "name": "@ericssonbroadcastservices/js-analytics-module",
  "version": "0.1.3",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "react-native": "./dist/index.js",
  "scripts": {
    "custom-publish": "npm publish",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "lint": "npx eslint .",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "exports": {
    ".": {
      "react-native": "./dist/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=18.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "react-native": ">=0.73.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/lodash.throttle": "^4.1.9",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "react-native": "0.72.x",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EricssonBroadcastServices/js-analytics-module.git"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "dependencies": {
    "@ericssonbroadcastservices/rbm-ott-sdk": "0.11.0",
    "@react-native-community/netinfo": "11.4.1",
    "bitmovin-player": "8.225.0",
    "lodash.throttle": "4.1.1",
    "mitt": "3.0.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  }
}